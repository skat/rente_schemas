<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:skat2012="http://skat.dk/begrebsmodel/2012/01/01/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2012/01/01/"
                 elementFormDefault="qualified"
                 attributeFormDefault="unqualified"
                 version="1.0">
         <xs:annotation>
               <xs:documentation>Indeholder en ustruktureret adresse/fritekstadresse. </xs:documentation>
            </xs:annotation>
         <xs:include schemaLocation="../types/IdentifikationNummerType.xsd"/>
         <xs:include schemaLocation="../types/AdresseAnvendelseKodeType.xsd"/>
         <xs:include schemaLocation="../types/AdresseLinieType.xsd"/>
         <xs:include schemaLocation="../types/DatoType.xsd"/>
         <xs:include schemaLocation="../types/UdenlandskKundeIdentType.xsd"/>
         <xs:element name="AlternativAdresseFortløbendeNummer"
                     type="skat2012:IdentifikationNummerType">
               
               <xs:annotation>
                  <xs:documentation>Angiver fortløbende nummerering ved flere adresser af samme type og på samme tidspunkt. Indikerer prioritet.</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseAnvendelseKode"
                     type="skat2012:AdresseAnvendelseKodeType">
               
               <xs:annotation>
                  <xs:documentation>Beskriver adresseringsmuligheder</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseAdresseLinie1" type="skat2012:AdresseLinieType">
               
               <xs:annotation>
                  <xs:documentation>Adresselinje 1</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseAdresseLinie2" type="skat2012:AdresseLinieType">
               
               <xs:annotation>
                  <xs:documentation>Adresselinie 2</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseAdresseLinie3" type="skat2012:AdresseLinieType">
               
               <xs:annotation>
                  <xs:documentation>Adresselinie 3</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseAdresseLinie4" type="skat2012:AdresseLinieType">
               
               <xs:annotation>
                  <xs:documentation>Adresselinie 4</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseAdresseLinie5" type="skat2012:AdresseLinieType">
               
               <xs:annotation>
                  <xs:documentation>Adresselinie 5</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseAdresseLinie6" type="skat2012:AdresseLinieType">
               
               <xs:annotation>
                  <xs:documentation>Adresselinie 6</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseAdresseLinie7" type="skat2012:AdresseLinieType">
               
               <xs:annotation>
                  <xs:documentation>Adresselinie 7</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseGyldigFra" type="skat2012:DatoType">
               
               <xs:annotation>
                  <xs:documentation>Specifik dato (uden start og slutangivelse).</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseGyldigTil" type="skat2012:DatoType">
               
               <xs:annotation>
                  <xs:documentation>Specifik dato (uden start og slutangivelse).</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AlternativAdresseReferenceNummer"
                     type="skat2012:UdenlandskKundeIdentType">
               
               <xs:annotation>
                  <xs:documentation>Anvendes kun for udenlandske kunder og er her optionel. Skal anvendes som en identifikation af kunden i forbindelse med kommunikation med udlandet. Referencenr. kan eksempelvis være et pasnummer. </xs:documentation>
               </xs:annotation>
            </xs:element>
      </xs:schema>
