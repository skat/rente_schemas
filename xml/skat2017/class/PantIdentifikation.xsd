	<xs:annotation>
		<xs:documentation>Institutterne bruger forskellige udtryk for pantidentifikation. Dette er en fællesbetegnelse.  Også omtalt som lånepantnummer og pantnummer.</xs:documentation>
	</xs:annotation>
	<xs:include schemaLocation="../types/Tekst15Type.xsd"/>
