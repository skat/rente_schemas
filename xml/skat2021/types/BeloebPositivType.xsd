<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:ekap2021="http://skat.dk/ekapital/2021/01/01" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://skat.dk/ekapital/2021/01/01" elementFormDefault="qualified" version="1.0">
	<xs:simpleType name="BeløbPositivType">
		<xs:annotation>
			<xs:documentation>Beløb angivet som decimaltal, fx. 1500,00</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([\+]){0,1}[0-9]{1,11}([,][0-9]{1,2}){0,1}"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
