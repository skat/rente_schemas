<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:ekap2018="http://skat.dk/ekapital/2018/01/01" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://skat.dk/ekapital/2018/01/01" elementFormDefault="qualified" version="1.0">
	<xs:simpleType name="TalHelType">
		<xs:annotation>
			<xs:documentation>Et positivt heltal, der kan repræsenterer værdier i intervallet 0 til 999.999.999.999.999.999</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:pattern value="([0-9])*"/>
			<xs:maxInclusive value="999999999999999999"/>
			<xs:minInclusive value="0"/>
			<xs:totalDigits value="18"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
